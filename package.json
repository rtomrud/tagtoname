{
  "name": "tagtoname",
  "version": "3.0.0",
  "description": "Renames audio files using the metadata tags",
  "keywords": [
    "audio",
    "file",
    "filename",
    "media",
    "metadata",
    "music",
    "rename",
    "tag"
  ],
  "homepage": "https://github.com/rtomrud/tagtoname#readme",
  "bugs": {
    "url": "https://github.com/rtomrud/tagtoname/issues"
  },
  "license": "MIT",
  "author": "Artem Rudenko <rtomrud@gmail.com>",
  "files": [
    "bin/cli.js",
    "index.js"
  ],
  "main": "index.js",
  "bin": "bin/cli.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rtomrud/tagtoname.git"
  },
  "scripts": {
    "lint": "eslint .",
    "start": "node bin/cli.js",
    "test": "nyc --reporter html tape \"test/**/*.js\" && nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "posttest": "npm run lint"
  },
  "dependencies": {
    "getopts": "2.2.5",
    "lockfile": "1.0.4",
    "music-metadata": "6.4.0",
    "standard-slugify": "2.0.1"
  },
  "devDependencies": {
    "eslint": "7.0.0",
    "eslint-config-pretty-strict": "6.0.0",
    "eslint-plugin-prettier": "3.1.3",
    "husky": "4.2.5",
    "nyc": "15.0.1",
    "prettier": "2.0.5",
    "tape": "5.0.0"
  },
  "engines": {
    "node": "10 || >= 12"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": [
      "./node_modules/eslint-config-pretty-strict/index.js"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "root": true
  },
  "eslintIgnore": [
    "coverage"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  }
}
